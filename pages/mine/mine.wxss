.mine-header navigator{
  display: flex;
  align-items: center;
  padding: 50rpx;
  background-color: #444;
  color: #fff;
}
.mine-header .loginname{
  margin-left: 16rpx;
  font-size: 32rpx;
}
.user-avatar{
  width: 100rpx;
  height: 100rpx;
}
.default-avatar{
  width: 100rpx;
  height: 100rpx;
  position: relative;
  border-radius: 50%;
  background: #fff;
  overflow: hidden;
}
.default-avatar::before, .default-avatar::after{
  content: "";
  display: block;
  position: absolute;
  left: 50%;
  background: #eee;
}
.default-avatar::before{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  top: 50%;
  margin-left: -20rpx;
  margin-top: -20rpx;
}
.default-avatar::after{
  width: 60rpx;
  height: 100%;
  border-radius: 20rpx;
  top: 50%;
  margin-left: -30rpx;
  margin-top: 20rpx;
}
